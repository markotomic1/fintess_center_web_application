
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id @default(uuid()) @map("user_id")
  name String
  surname String
  username String @unique
  email String @unique 
  password String @unique
  role Role @default(USER)
}
enum Role{
  ADMIN
  TRAINER
  USER
}